{
  "manifest_version": 3,
  "name": "Website Blocker",
  "version": "3.0",
  "description": "Blocks a list of specified websites.",
  "permissions": [
    "storage",
    "webNavigation",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html"
  }
}
